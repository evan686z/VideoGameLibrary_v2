@model Microsoft.AspNet.Identity.EntityFramework.IdentityRole

@{
    ViewBag.Title = "User Role Details";
}

<h2>User Role Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

    </dl>
</div>
<div class="form-group">
    <input type="button" value="Back" class="btn btn-primary" onclick="location.href='@Url.Action("Index", "IdentityRole")'" />
    <input type="button" value="Edit" class="btn btn-warning" onclick="location.href='@Url.Action("Edit", "IdentityRole", new { id = Model.Id })'" />
    <input type="button" value="Delete" class="btn btn-danger" onclick="location.href='@Url.Action("Delete", "IdentityRole", new { id = Model.Id })'" />
</div>
